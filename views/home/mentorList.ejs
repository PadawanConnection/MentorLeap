<div class='container'>
  <div>
    <h1>Mentors</h1>
  </div>
  <div>
    <table class="table">
    <tr>
      <th>User Name</th>
    </tr>

    <% _.each(users, function(user){ %>
    <tr data-id="<%= user.id %>" data-model='user'>
      <td><%= user.username %></td>
      <td><a href="user/show/<%=user.id%>" class='btn btn-small btn-primary'>Show</a></td>
    </tr>
    <% })%>
    </table>
  </div>
</div>
